
@{
    ViewBag.Title = "Result";
}

<h2>Result</h2>

@using (Html.BeginForm())
{
    <div class="row">
        <div class="col-lg-3"></div>
        <div class="col-lg-6">
            <hr />

            @if (ViewBag.QrResult != null)
            {
                <p>
                    <br />
                    Decoded code: <br />
                    <font color="blue">@Html.Raw(@ViewBag.QrResult)</font>
                </p>
            }

        </div>
    </div>
}